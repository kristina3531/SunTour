.header
  background-color main-color
  padding 30px 0
  color #fff
  .container
    display flex
    align-items center
  &-logo
    margin-right 20px
    &__img
      width 179px
      height 44px
  &-heading
    font-size 16px
    font-weight 400
  &-phone
    margin-left auto
    font-size 30px
  &-feedback
    margin-left 25px
    border-bottom 1px dashed #fff
    transition all .3s
    &:hover
      border-color transparent
  .social
    margin-left 45px
    
    
      